{
	"name": "quikfill-be-node",
	"version": "1.0.0",
	"description": "Backend service for QuikFIll",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"typeorm": "npx typeorm",
		"start": "node src/index.js",
		"dev": "nodemon src/index.js",
		"migration:generate": "npx typeorm migration:generate --outputJs ./src/lib/database/migrations/migration -d ./src/lib/database/datasource.js",
		"migration:show": "npx typeorm migration:show -d src/lib/database/datasource.js",
		"migration:create": "npx typeorm migration:create ./src/lib/database/migrations/migration",
		"migration:run": "npx typeorm migration:run -d ./src/lib/database/datasource.js",
		"debug": "node --inspect --watch src/index.js",
		"rollback:db:trx": "node src/lib/database/rollback-trx.js"
	},
	"repository": "https://github.com/badirusubomi/quikfill-be-node.git",
	"author": "Olasubomi O. Badiru <badirusubomi@icloud.com> & Timilehin Jacobs <timij90@gmail.com>",
	"license": "MIT",
	"private": false,
	"dependencies": {
		"@aws-sdk/client-s3": "^3.848.0",
		"@aws-sdk/lib-storage": "^3.848.0",
		"@google-cloud/aiplatform": "^4.4.0",
		"@google/generative-ai": "^0.24.1",
		"@pinecone-database/pinecone": "^6.1.1",
		"aws-sdk": "^2.1692.0",
		"aws-sdk-js-v3": "^0.0.2",
		"axios": "^1.10.0",
		"cors": "^2.8.5",
		"dotenv": "^17.2.0",
		"express": "^5.1.0",
		"googleapis": "^153.0.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.1",
		"multer": "^2.0.1",
		"multer-s3": "^3.0.1",
		"nodemon": "^3.1.10",
		"pdf-parse": "^1.1.1",
		"pg": "^8.16.3",
		"pinecone-client": "^2.0.0",
		"prisma": "^6.12.0",
		"reflect-metadata": "^0.2.2",
		"supabase": "^2.31.4",
		"tiktoken": "^1.0.21",
		"typeorm": "^0.3.25"
	}
}
